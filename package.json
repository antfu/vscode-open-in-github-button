{
  "publisher": "antfu",
  "name": "open-in-github-button",
  "displayName": "Open in GitHub Button",
  "version": "0.1.4",
  "private": true,
  "packageManager": "pnpm@10.19.0",
  "description": "Add a button to go to the GitHub on the status bar",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/vscode-open-in-github-button#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/vscode-open-in-github-button"
  },
  "bugs": {
    "url": "https://github.com/antfu/vscode-open-in-github-button/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/antfu"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "LICENSE.md",
    "dist/*",
    "res/*"
  ],
  "engines": {
    "vscode": "^1.105.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [],
    "configuration": {
      "type": "object",
      "title": "Project Name In StatusBar extension configuration",
      "properties": {
        "open-in-github-button.align": {
          "type": "string",
          "enum": [
            "left",
            "right"
          ],
          "default": "left",
          "description": "Defines The alignment of the label, requires restart of vscode"
        },
        "open-in-github-button.alignPriority": {
          "type": "number",
          "default": 0,
          "description": "Defines priority of the label. Higher values mean the label should be shown more to the left, requires restart of vscode"
        }
      }
    }
  },
  "extensionPack": [
    "fabiospampinato.vscode-open-in-github"
  ],
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "ext:publish": "vsce publish --no-dependencies",
    "package": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@antfu/ni": "^27.0.0",
    "@types/node": "^24.9.1",
    "@types/vscode": "^1.105.0",
    "@vscode/vsce": "^3.6.2",
    "bumpp": "^10.3.1",
    "eslint": "^9.38.0",
    "esno": "^4.8.0",
    "ovsx": "^0.10.6",
    "pnpm": "^10.19.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.2",
    "vsxpub": "^0.1.2"
  }
}
